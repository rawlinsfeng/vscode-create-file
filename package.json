{
	"name": "vscode-create-file",
	"displayName": "Create File",
	"description": "Create new (template) files and directories based on a json file or from the command panel.",
	"version": "0.0.1",
	"publisher": "rawlinsfeng",
	"engines": {
		"vscode": "^1.53.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:vscode-create-file.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vscode-create-file.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.63.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4",
		"@vscode/test-electron": "^1.6.2"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/rawlinsfeng/vscode-create-file"
	},
	"bugs": {
		"url": "https://github.com/rawlinsfeng/vscode-create-file/issues"
	},
	"icon": "assets/images/logo.png"
}
